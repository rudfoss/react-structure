{
	"$schema": "./node_modules/nx/schemas/nx-schema.json",
	"npmScope": "react-structure",
	"affected": {
		"defaultBase": "main"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/nx-cloud",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"accessToken": "YzdjN2ZlM2YtZjcwYy00YjcwLTkzMjEtYTY1NDI0N2IwZTdlfHJlYWQtd3JpdGU="
			}
		}
	},
	"targetDefaults": {
		"build": {
			"dependsOn": ["^build"]
		}
	},
	"workspaceLayout": {
		"appsDir": "apps",
		"libsDir": "libs"
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"style": "@emotion/styled",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "@emotion/styled"
			},
			"library": {
				"style": "@emotion/styled",
				"linter": "eslint"
			}
		}
	}
}
